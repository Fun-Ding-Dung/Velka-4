{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
        "default_icon": "pragrest.png"
    },
    "content_scripts": [
        {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"],
        "css": ["styles.css"]
        }
    ],
    "background": {
    "service_worker": "background.js"
    }
  }
